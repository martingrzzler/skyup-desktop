{
  "productName": "SkyUp",
  "version": "1.0.5",
  "identifier": "skytraxx.org.Skyup",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "SkyUp",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "category": "Utility",
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "macOS": {
      "entitlements": "./Entitlements.plist",
      "files": {
        "embedded.provisionprofile": "SkyUp.provisionprofile"
        }
      },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZCNkUyQUIyNDUzRTJGMTAKUldRUUx6NUZzaXB1KzluaS9pVjJ1elRNeVlwSjNmRVd0QndCSnFrakZ5RFBlVnI0Q1NQUTlZNkQK",
      "endpoints": ["https://www.skytraxx.org/skyup/skyup_update.json"],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
